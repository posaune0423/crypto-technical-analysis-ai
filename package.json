    "drizzle-orm": "^0.40.1",
    "express": "^4.19.2",
    "node-schedule": "^2.1.1",
    "sqlite3": "^5.1.7",
    "technicalindicators": "^3.1.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.2",
    "ws": "^8.16.0"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.12",
    "drizzle-kit": "^0.30.5",
    "prettier": "^3.5.3",
